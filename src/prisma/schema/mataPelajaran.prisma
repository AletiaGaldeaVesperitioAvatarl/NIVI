model MataPelajaran {
  id        Int      @id @default(autoincrement())
  nama      String
  kode      String

  kelasId   Int
  pengajarId Int

  kelas     Kelas   @relation(fields: [kelasId], references: [id])
  pengajar  User    @relation(fields: [pengajarId], references: [id])

  tugas     Tugas[]

  createdAt DateTime @default(now())

  @@unique(kode)
}
